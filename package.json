    "css-loader": "^3.2.0",
    "eslint": "^5.12.0",
    "eslint-config-4catalyzer": "^0.7.0",
    "eslint-config-4catalyzer-jest": "^1.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.1.2",
